{
  "name": "@tapendium/testcontainers-dynamodb",
  "version": "0.0.2",
  "author": "Tapendium",
  "main": "dist/index",
  "types": "src/index",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "format": "prettier --ignore-unknown --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "validate": "run-p --continue-on-error --print-name format:check typecheck",
    "test": "vitest --run"
  },
  "homepage": "https://github.com/tapendium/testcontainers-dynamodb#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapendium/testcontainers-dynamodb.git"
  },
  "keywords": [
    "testing",
    "testcontainers",
    "dynamodb"
  ],
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.529.1",
    "@aws-sdk/lib-dynamodb": "^3.529.1",
    "@aws-sdk/util-dynamodb": "^3.529.1"
  },
  "devDependencies": {
    "@tapendium/prettier-config-apollo": "^0.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "testcontainers": "^10.7.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
